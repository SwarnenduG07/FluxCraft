
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model userAPI {
   id         String  @id @default(cuid())
   userId     String      @unique
   count      Int         @default(0)
   createdAi  DateTime    @default(now())
   updatedAi  DateTime    @updatedAt
}



